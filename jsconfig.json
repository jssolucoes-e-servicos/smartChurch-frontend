{
  "compilerOptions": {
    "target": "ES2022",
    "baseUrl": ".",
    "paths": {
      "~/assets/*": [
        "src/assets/*"
      ],
      "~/components/*": [
        "src/components/*"
      ],
      "~/configs/*": [
        "src/configs/*"
      ],
      "~/contexts/*": [
        "src/contexts/*"
      ],
      "~/core/*": [
        "src/core/*"
      ],
      "~/templates/*": [
        "src/templates/*"
      ],
      "~/partials/*": [
        "src/partials/*"
      ],
      "~/services/*": [
        "src/services/*"
      ],
      "~/styles/*": [
        "src/styles/*"
      ],
    }
  }
}